
struct VS_OUTPUT
{
    float4 Position   : SV_Position;
};

int angle;

VS_OUTPUT main( in float4 vPosition : POSITION, in int coord: TEXCOORD,
in int coord2 : TEXCOORD1 )
{
    VS_OUTPUT Output;
	float4 tempArray[2] = {float4(0, 0, 0, 0), float4(0, 0, 0, 0)};
	float4 tempArray2[2] = {float4(0, 0, 0, 0), float4(0, 0, 0, 0)};

    tempArray[coord] = 1;
	tempArray[coord + 1] = 0;
	
	tempArray2[coord2] = 1;
	tempArray2[coord2+2] = 0;

    Output.Position.xy = tempArray[coord & 1].xx;
    Output.Position.zw = tempArray2[coord2 & 1].xx;
    
    return Output;
}


